{
  "name": "@orbit-codes/nestjs-mcp",
  "version": "1.0.0",
  "description": "A NestJS package to integrate MCP",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": "git@github.com:orbit-codes/nestjs-mcp.git",
  "author": "Orbit Codes",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src ./test",
    "format:check": "biome format ./src ./test",
    "check": "biome check ./src ./test",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/core": "^11.0.12",
    "@nestjs/testing": "^11.0.12",
    "@swc/core": "^1.11.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.1.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "zod": "^3.22.4"
  }
}
